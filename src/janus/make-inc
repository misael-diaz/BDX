INC = -I../../include

UTIL_H     = ../../include/util.h
ID_H       = ../../include/ID.h
VECTOR_H   = ../../include/Vector.h
OBJECT_H   = ../../include/BDXObject.h
PARTICLE_H = ../../include/Particle.h
SPHERE_H   = ../../include/Sphere.h
JANUS_H    = ../../include/Janus.h
